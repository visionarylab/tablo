{
  "name": "Tablo",
  "short_name": "Tablo",
  "description": "New tab extension",
  "manifest_version": 2,
  "minimum_chrome_version": "0",
  "version": "1.0.0",
  "offline_enabled": true,
  "web_accessible_resources": [
    "index.html"
  ],
  "icons": {
    "512": "/assets/icons/android-chrome-512x512.png",
    "192": "/assets/icons/android-chrome-192x192.png",
    "32": "/assets/icons/favicon-32x32.png",
    "16": "/assets/icons/favicon-16x16.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "matches": [ "chrome://*/*" ],
  "content_security_policy": "script-src 'self' 'unsafe-inline' unsafe-eval'; object-src 'self' 'sha256-5As4+3YpY62+l38PsxCEkjB1R4YtyktBtRScTJ3fyLU='",
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "history",
    "storage",
    "http://www.moma.org/*",
    "http://www.tate.org.uk/*",
    "chrome://apps/",
    "chrome://bookmarks/",
    "chrome://downloads/",
    "chrome://history/",
    "chrome://setting/s",
    "chrome://newtab/#games"
  ]
}