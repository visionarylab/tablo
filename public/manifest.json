{
  "name": "Tablo",
  "short_name": "Tablo",
  "description": "New tab extension",
  "manifest_version": 2,
  "minimum_chrome_version": "0",
  "version": "1.0.0",
  "offline_enabled": true,
  "web_accessible_resources": ["index.html"],
  "icons": {
    "128": "favicon.ico",
    "16": "favicon.ico",
    "48": "favicon.ico"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-inline' unsafe-eval'; object-src 'self' 'sha256-5As4+3YpY62+l38PsxCEkjB1R4YtyktBtRScTJ3fyLU='",
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "history",
    "storage",
    "http://www.moma.org/*",
    "http://www.tate.org.uk/*"
  ]
}
